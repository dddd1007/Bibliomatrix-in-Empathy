@article{ ISI:000376829700005,
Author = {Ryu, Hye-Ran and Bang, Kyung-Sook},
Title = {{A Validation Study of the Korean Version of the Jefferson Empathy Scale
   for Health Professionals for Korean Nurses}},
Journal = {{JOURNAL OF KOREAN ACADEMY OF NURSING}},
