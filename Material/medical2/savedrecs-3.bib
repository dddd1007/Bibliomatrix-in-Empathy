@article{ ISI:000181718800014,
Author = {Wheat, JR and Brandon, JE and Carter, LR and Leeper, JD and Jackson, JR},
Title = {{Premedical education: The contribution of small local colleges}},
Journal = {{JOURNAL OF RURAL HEALTH}},
Year = {{2003}},
Volume = {{19}},
Number = {{2}},
Pages = {{181-189}},
Month = {{SPR}},
Abstract = {{Context: Small local colleges may be sources of medical students with
   the gender, ethnicity, and background that promote identity with and
   empathy for underserved populations. Purpose: This study examine the
   impact of attendance at these premedical colleges on outcomes of medical
   education. Methods: Data for 2508 matriculated to the University of
   Alabama School of Medicine, a state-support medical school, were
   examined according to premedical colleges attended. Findings: Medical
   students who had graduated from small local college were more diverse in
   gender, race, and rural background than other students. They had
   slightly lower academic performance in medical school, were more likely
   to drop out (10.6\% versus 5.2\% overall), and were more likely to
   locate in rural areas of the host state. Conclusions: Small local
   colleges may be rich sources of student diversity and medical students
   who choose rural practice, outcomes that are gained at a cost in terms
   of drop-out rate. Compared with other students, minor differences in
   performance and larger differences in the drop-out rate raise the
   question of cultural context and social support during medical school as
   points for intervention.}},
