@article{ ISI:000360992600011,
Author = {Ventres, William B. and Frankel, Richard M.},
Title = {{Shared Presence in Physician-Patient Communication: A Graphic
   Representation}},
Journal = {{FAMILIES SYSTEMS \& HEALTH}},
