@article{ ISI:000167146700008,
Author = {Katic, M and Budak, A and Ivankovic, D and Mastilica, M and Lazic, D and
   Babic-Banaszak, A and Matkovic, V},
Title = {{Patients' views on the professional behaviour of family physicians}},
