@article{ ISI:000185349600016,
Author = {Shapiro, J and Hunt, L},
Title = {{All the world's a stage: the use of theatrical performance in medical
   education}},
Journal = {{MEDICAL EDUCATION}},
Year = {{2003}},
Volume = {{37}},
Number = {{10}},
Pages = {{922-927}},
Month = {{OCT}},
Abstract = {{Purpose Student exposure to illness-related theatrical performances
   holds intriguing educational possibilities. This project explored uses
   of theatrical performance within the context of medical education.
   Method Two 1-person shows, dramatically addressing AIDS and ovarian
   cancer, were presented to audiences totalling approximately 150 medical
   students, faculty, community doctors, staff and patients.
   Results Evaluations for both performances indicated increased
   understanding of the illness experience and greater empathy for
   patients. They also showed that respondents obtained additional insights
   into patient care issues, and developed new ways of thinking about their
   situations.
   Conclusions Presenting illness-related dramatic performances as an
   adjunct method of enhancing empathy and insight toward patients in a
   self-selected group of students, doctors, staff and patients was
   successful. Although this approach might not be effective with all
   learners, those who participated felt they gained important insights
   into the nature of the patient experience.}},
