@article{ ISI:000310874900013,
Author = {Ozcan, C. T. and Oflaz, F. and Bakir, B.},
Title = {{The effect of a structured empathy course on the students of a medical
   and a nursing school}},
Journal = {{INTERNATIONAL NURSING REVIEW}},
Year = {{2012}},
