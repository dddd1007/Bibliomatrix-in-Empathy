@article{ ISI:000264701200028,
Author = {Hainer, Vojtech and Toplak, Hermann and Mitrakou, Asimina},
Title = {{Treatment Modalities of Obesity What fits whom?}},
Journal = {{DIABETES CARE}},
Year = {{2008}},
