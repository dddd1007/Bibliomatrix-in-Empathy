@article{ ISI:000177062800005,
Author = {Mangione, S and Kane, GC and Caruso, JW and Gonnella, JS and Nasca, TJ
   and Hojat, M},
Title = {{Assessment of empathy in different years of internal medicine training}},
Journal = {{MEDICAL TEACHER}},
Year = {{2002}},
Volume = {{24}},
Number = {{4}},
Pages = {{370-373}},
Month = {{JUL}},
Abstract = {{The operational measurement of physician empathy, as well as the
   question of whether empathy could change at different levels of medical
   education, is of interest to medical educators. To address this issue,
   98 internal medicine residents from all 3 years of training were
   studied. The Jefferson Scale of Physician Empathy was administered, and
   residents' empathy scores correlated with ratings on humanistic
   attributes made by postgraduate program directors. No statistically
   significant differences in scores were found among residents of
   different training levels. Empathy scores remained also stable during
   internship (test-retest reliability = 0.72). Correlation between empathy
   and ratings on humanism was 0.17. Thus, the findings suggest that
   empathy is a relatively stable trait that is not easily amenable to
   change in residency training programs. The issue of whether targeted
   educational activities for the purpose of cultivating empathy can
   improve empathy scores awaits empirical scrutiny.}},
