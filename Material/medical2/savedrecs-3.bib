@article{ ISI:000073884800004,
Author = {Henderson, JV},
Title = {{Comprehensive, technology-based clinical education: The ``virtual
   practicum{''}}},
Journal = {{INTERNATIONAL JOURNAL OF PSYCHIATRY IN MEDICINE}},
Year = {{1998}},
Volume = {{28}},
Number = {{1}},
Pages = {{41-79}},
