@article{ ISI:000303384700021,
Author = {Wear, Delese and Zarconi, Joseph and Garden, Rebecca and Jones, Therese},
Title = {{Reflection in/and Writing: Pedagogy and Practice in Medical Education}},
Journal = {{ACADEMIC MEDICINE}},
