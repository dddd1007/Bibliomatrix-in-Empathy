@article{ ISI:000248008100008,
Author = {Dow, Alan W. and Leong, David and Anderson, Aaron and Wenzel, Richard P.
   and VCU Theater-Med Team},
Title = {{Using theater to teach clinical empathy: A pilot study}},
Journal = {{JOURNAL OF GENERAL INTERNAL MEDICINE}},
