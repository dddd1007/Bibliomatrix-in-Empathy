@article{ ISI:000338189900014,
Author = {Olthuis, Gert and Leget, Carlo and Grypdonck, Mieke},
Title = {{Why shared decision making is not good enough: lessons from patients}},
Journal = {{JOURNAL OF MEDICAL ETHICS}},
Year = {{2014}},
Volume = {{40}},
Number = {{7}},
Pages = {{493-495}},
Month = {{JUL}},
Abstract = {{A closer look at the lived illness experiences of medical professionals
   themselves shows that shared decision making is in need of a logic of
   care. This paper underlines that medical decision making inevitably
   takes place in a messy and uncertain context in which sharing
   responsibilities may impose a considerable burden on patients. A better
   understanding of patients' lived experiences enables healthcare
   professionals to attune to what individual patients deem important in
   their lives. This will contribute to making medical decisions in a good
   and caring manner, taking into account the lived experience of being
   ill.}},
