@article{ ISI:000284778500002,
Author = {Parsons, Annie and Hooker, Claire},
Title = {{Dignity and Narrative Medicine}},
Journal = {{JOURNAL OF BIOETHICAL INQUIRY}},
Year = {{2010}},
Volume = {{7}},
Number = {{4}},
Pages = {{345-351}},
Month = {{DEC}},
Abstract = {{Critiques of the dehumanising aspects of contemporary medical practice
   have generated increasing interest in the ways in which health care can
   foster a holistic sense of wellbeing. We examine the relationship
   between two areas of this humanistic endeavour: narrative and dignity.
   This paper makes two simple arguments that are intuitive but have not
   yet been explored in detail: that narrative competence of carers is
   required for maintaining or recreating dignity, and that dignity
   promotion in health care practice is primarily narrative in form. The
   multiple meanings that dignity has in a person's life are what give the
   concept power and can only be captured by narrative. This has
   implications for health care practice where narrative work will be
   increasingly required to support patient dignity in under-resourced and
   over-subscribed health care systems.}},
