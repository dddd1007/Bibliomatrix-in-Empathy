@article{ ISI:000279634200001,
Author = {Greenburg, David L. and Durning, Steven J. and Cruess, David L. and
   Cohen, Daniel M. and Jackson, Jeffrey L.},
Title = {{The Prevalence, Causes, and Consequences of Experiencing a Life Crisis
   During Medical School}},
Journal = {{TEACHING AND LEARNING IN MEDICINE}},
Year = {{2010}},
