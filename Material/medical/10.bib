@article{ ISI:000313774500007,
Author = {Seguret, Sylvie and Hubert, Philippe and Golse, Bernard},
Title = {{ANALYSIS OF PRACTICES USED IN A PEDIATRIC INTENSIVE CARE UNIT. THE WORK
   OF A PSYCHOLOGIST ATTACHED TO THE MEDICAL TEAM}},
Journal = {{PSYCHIATRIE DE L ENFANT}},
Year = {{2012}},
Volume = {{55}},
Number = {{2}},
Pages = {{575-605}},
Abstract = {{The Pediatric Intensive Care Unit of Necker Hospital for Sick Children
   cares for severely ill newborn infants and children. Ethical dilemmas
   about whether to continue care, reduce or abandon treatment occur daily.
   A partnership with the parents represents an essential aspect of the
   medical and paramedical care provided. A psychologist intervenes in this
   context to support the health care personnel and to analyse the
   functioning of the system. A study was carried out to assess how useful
   family interviews were when announcing the worsening condition of the
   hospitalized child. The idea of re-enacting these interviews with the
   medical team which had been involved, led to a new way of conducting
   such interviews. The work of the staff members who had taken part in
   this re-enacting was assessed. Analysing these practices as well as
   listening to what the staff had to say during informal exchanges enabled
   the team to express their unvoiced concerns. Using such techniques,
   practices have been improved and in this paper, we propose to offer one
   theoretical elaboration of this point. The original role played here by
   a psychologist who is specifically devoted to listening to the staff of
   a highly specialised hospital service, dealing daily with vital, ethical
   questions, could well be continued and assessed at regular intervals.}},
