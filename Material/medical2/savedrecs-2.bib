@article{ ISI:000270026000011,
Author = {West, Colin P. and Tan, Angelina D. and Habermann, Thomas M. and Sloan,
   Jeff A. and Shanafelt, Tait D.},
Title = {{Association of Resident Fatigue and Distress With Perceived Medical
   Errors}},
